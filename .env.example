# Allowed CORS origins (comma-separated)
# Example: https://yourdomain.com,https://app.yourdomain.com
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Admin wallet addresses (comma-separated)
# These wallets have access to admin endpoints
ADMIN_WALLETS=ABjnax7QfDmG6wR2KJoNc3UyiouwTEZ3b5tnTrLLyNSp,BRofRUhr1k9fqcpjncBR3xHPxWNW2tHYfYHFPxtqSbhv

# Treasury wallet private key (holds vesting funds)
# JSON array format: [1,2,3,4,...] (64 bytes)
# Base58 format: 5Jv8...xyz
TREASURY_PRIVATE_KEY=your_treasury_private_key_here

# Admin wallet private key (for administrative operations)
# If not set, defaults to TREASURY_PRIVATE_KEY
# Can be set to the same value as TREASURY_PRIVATE_KEY
ADMIN_PRIVATE_KEY=your_admin_private_key_here

# Custom token mint address
CUSTOM_TOKEN_MINT=your_custom_token_mint_address

# Fee recipient wallet
FEE_WALLET=your_fee_wallet_address

# NFT Collection address (optional - if not set, counts all NFTs)
NFT_COLLECTION_ADDRESS=your_nft_collection_address

# Supabase configuration
